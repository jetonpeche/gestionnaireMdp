<MudDialog>
    <TitleContent>
        <MudText Typo="Typo.h6">Catégorie</MudText>
    </TitleContent>
    <DialogContent>
        <EditForm EditContext="EditContext">

            <MudTextField T="string" @bind-Value="Categorie.Nom" Variant="Variant.Outlined" Label="Nom" MaxLength="100" Required="true" />

        </EditForm>
    </DialogContent>
    <DialogActions>
        <MudIconButton OnClick="Fermer" Variant="Variant.Filled" Color="Color.Error" Icon="@Icons.Material.Filled.Close" />
        <MudIconButton OnClick="FormEnvoyerAsync" Variant="Variant.Filled" Color="Color.Primary" Icon="@Icons.Material.Filled.Check" />
    </DialogActions>
</MudDialog>
